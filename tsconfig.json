{
  "compilerOptions": {
    "module": "amd",
        "sourceMap": true,
        "target": "es5",
        "outDir": "public/js",
        "lib": [
            "dom",
            "es2015"
        ],
        "moduleResolution": "node",
    // "target": "esnext",
    // "module": "esnext",
    // "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    // "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    // "lib": [
    //   "esnext",
    //   "dom"
    // ],
    "types": [
      "vite/client",
      "jest",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "test/**/*.ts"
, "src/as/assembly/tsconfig.json"  ]
}